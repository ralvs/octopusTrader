{
  "extends": [
    "wesbos"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "avoid",
        "jsxSingleQuote": true,
        "singleQuote": true,
        "semi": false,
        "printWidth": 110
      }
    ],
    "no-case-declarations": 0
  }
}
